{
  "name": "@rom98m/utils",
  "version": "1.1.0",
  "description": "Various TS utils",
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "rm -rf dist && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "test": "npm run build && jest --clearCache && node --experimental-vm-modules node_modules/.bin/jest",
    "test:win": "echo \"Use same command as for \\`npm test\\` but adjust extension of the \\`n_m/.bin/jest\\`: either \\`.cmd\\` or \\`.ps1\\`\""
  },
  "keywords": [
    "utils",
    "random",
    "format",
    "string",
    "async",
    "batch"
  ],
  "author": "Roman Melnyk <https://melnyk.site>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rom98m/ts-utils.git"
  },
  "bugs": {
    "url": "https://github.com/rom98m/ts-utils/issues",
    "email": "rom98m@gmail.com"
  },
  "license": "UNLICENSED",
  "devDependencies": {
    "@jest/globals": "30.2.0",
    "jest": "30.2.0",
    "ts-jest": "29.4.6"
  }
}
